{
  "name": "Switch",
  "description": "Home frontend",
  "version": "1.0.0",
  "author": "Riccardo Di Virgilio",
  "license": "MIT",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/icon.icns --prune=true --out=builds"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "^2.0.0",
    "electron-packager": "^12.1.1",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "rangetouch": "^1.0.5",
    "rfuncs": "^1.2.0",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "vue": "^2.5.16",
    "vue-loader": "^13.0.5",
    "vue-native-websocket": "^2.0.5",
    "vue-template-compiler": "^2.4.4",
    "vue-websocket": "^0.2.2",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "websocket": "^1.0.25"
  }
}
